{
  "name": "lightning-flow-scanner",
  "version": "2.36.1",
  "bugs": "https://github.com/Lightning-Flow-Scanner/lightning-flow-scanner-sfdx/issues",
  "dependencies": {
    "@oclif/core": "^4.2.2",
    "@salesforce/core": "^8.8.0",
